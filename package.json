{
    "name": "nosfir",
    "version": "1.0.0",
    "description": "Nosfir WordPress Theme",
    "scripts": {
        "watch": "sass --watch style.scss:style.css assets/sass:assets/css --style expanded",
        "build": "sass style.scss:style.css assets/sass:assets/css --style compressed",
        "dev": "sass style.scss:style.css assets/sass:assets/css --style expanded --source-map"
    },
    "devDependencies": {
        "sass": "^1.69.0"
    }
}